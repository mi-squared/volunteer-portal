container_commands:
   00-make-env:
      command: "cp /var/app/current/service/.env-ci /var/app/current/service/.env"
   01-migrateSeed:
      command: "cd /var/app/current/service && php artisan migrate --force"
   02-seed:
      command: "cd /var/app/current/service && php artisan db:seed --force"
